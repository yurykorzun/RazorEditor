    <link href="@Url.Content("~/Content/font-awesome.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/select2/select2.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/select2/select2-bootstrap.css")" rel="stylesheet" />
    <style>
